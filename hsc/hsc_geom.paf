#-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#
# Arrangement of amps (readout channels)
#
Ccd: {
    pixelSize: 1.0                      # pixel size (we're using pixels, not mm)
    offsetUnit: "pixels"
    ptype: "default"
    nCol: 4                             # number of columns of amps; 2 == Left and Right
    nRow: 1                             # number of rows of amps
    Amp: {
        serial: 1
        ptype: "default"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 0 0                      # Location of Amp in CCD thought of as a grid of amplifiers
    }
    Amp: {
        serial: 2
        ptype: "default"
        flipLR: true
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 1 0                      # Location of Amp in CCD thought of as a grid of amplifiers
    }
    Amp: {
        serial: 3
        ptype: "default"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 2 0                      # Location of Amp in CCD thought of as a grid of amplifiers
    }
    Amp: {
        serial: 4
        ptype: "default"
        flipLR: true
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 3 0                      # Location of Amp in CCD thought of as a grid of amplifiers
    }
}

#-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#
# Pixel Format of Scientific CCDs (without AG and defocused CCDs)
#    * For the full frame in (0, 0)-originated coordinates
#    * HSC Raw FITS data have only a single common pixel format
#
Amp: {
    ptype: "default"
    datasec: 8 49 519 4224
    biassec: 520 49 535 4224

    ewidth: 536    # 512(eff)+8(pre-blank)+16(overscan)
    eheight: 4241  # 4176(eff)+49(near-the-amp pix)+16(overscan)
}

# C.f., Keywords of HSC pixel format in (1,1)-originated coordinates
# T_OSMN11=                  521 / MIN pixel of x-overscan region for ch1
# T_OSMX11=                  536 / MAX pixel of x-overscan region for ch1
# T_EFMN11=                    9 / MIN pixel of x-effective range for ch1
# T_EFMX11=                  520 / MAX pixel of x-effective range for ch1
# T_OSMN12=                 4226 / MIN pixel of y-overscan region for ch1
# T_OSMX12=                 4241 / MAX pixel of y-overscan region for ch1
# T_EFMN12=                   50 / MIN pixel of y-effective range for ch1
# T_EFMX12=                 4225 / MAX pixel of y-effective range for ch1

