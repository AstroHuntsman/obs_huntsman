#<?cfg paf policy ?>

cameraDescription:	"suprimecam/description/Full_Suprime_geom.paf"
defectPath:		"suprimecam/description/defects"
filterDescription:	"suprimecam/description/SuprimeFilters.paf"

rawTemplate:		"%(field)s/%(dateObs)s/%(mystery)s/%(filter)s/SUPA%(visit)07d%(ccd)1d.fits"

flat: {
	template: "FLAT/%(dateObs)s/%(filter)s/%(mystery)s/FLAT-%(visit)07d%(ccd)1d.fits"
	query: "SELECT * FROM flat WHERE filter = '%(filter)s' AND ccd = %(ccd)d AND DATE('%(taiObs)s') BETWEEN DATE(validStart) AND DATE(validEnd)"
}


postISRTemplate:	"postISR/v%(visit)d-f%(filter)s/c%(ccd)02d-a%(amp)d.fits"
postISRCCDTemplate:	"postISRCCD/v%(visit)d-f%(filter)s/c%(ccd)02d.fits"
visitimTemplate:	"visitim/v%(visit)d-f%(filter)s/c%(ccd)02d.fits"
psfTemplate:		"psf/v%(visit)d-f%(filter)s/c%(ccd)02d.boost"
calexpTemplate:		"calexp/v%(visit)d-f%(filter)s/c%(ccd)02d.fits"
srcTemplate:		"src/v%(visit)d-f%(filter)s/c%(ccd)02d.boost"
sourceHistTemplate:	"results/st%(skyTile)d/sourceHist.fits"
badSourceHistTemplate:	"results/st%(skyTile)d/badSourceHist.fits"
sourceTemplate:		"results/st%(skyTile)d/source.boost"
badSourceTemplate:	"results/st%(skyTile)d/badSource.boost"
invalidSourceTemplate:	"results/st%(skyTile)d/invalidSource.boost"
objectTemplate:		"results/st%(skyTile)d/object.boost"
badObjectTemplate:	"results/st%(skyTile)d/badObject.boost"


