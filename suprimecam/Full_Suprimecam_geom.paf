Camera: { 
    name: "Subaru SuprimeCam" 
    serial: 1
    nCol: 1 
    nRow: 1 
    Raft: { 
        name: "R:0,0" 
        serial: -1 
        index: 0 0 
        offset: 0.0 0.0 
    }
    #Distortion: {
    #    active: "RadialPolyDistortion"
    #    RadialPolyDistortion: {
    #     coeffs: 0.0 1.0 7.16417e-08 3.03146e-10 5.69338e-14 -6.61572e-18
    #    }
    #}
} 
Amp: { 
    # Amp characteristics of CCDs on the bottom row: 8, 9, 5, 4, 3
    ptype: "bottom"
    datasec: 8 48 519 4224
    biassec: 520 48 567 4224
    ewidth: 568
    eheight: 4273
} 
Amp: { 
    # Amp characteristics of CCDs on the top row: 6, 7, 2, 1, 0
    ptype: "top"
    datasec: 8 48 519 4224
    biassec: 520 48 567 4224
    ewidth: 568
    eheight: 4273
} 
Ccd: {
    # Bottom row: CCDs 8, 9, 5, 4, 3
    ptype: "bottom"
    pixelSize: 10.0e-3  # mm
    nCol: 4
    nRow: 1 
    Amp: { 
        serial: 1
        ptype: "bottom"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 0 0 
    } 
    Amp: { 
        serial: 2
        ptype: "bottom"
        flipLR: true
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 1 0 
    } 
    Amp: { 
        serial: 3
        ptype: "bottom"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 2 0 
    } 
    Amp: { 
        serial: 4
        ptype: "bottom"
        flipLR: true
        nQuarter: 0
        hdu: 0
        diskCoordSys: "sensor"
        index: 3 0 
    } 
}
Ccd: {
    # Top row: CCDs 6, 7, 2, 1, 0
    # We have to undo the nQuarter=2 in the CCD.
    # We do this by applying another nQuarter, and numbering in the opposite direction
    ptype: "top"
    pixelSize: 10.0e-3  # mm
    nCol: 4
    nRow: 1 
    Amp: { 
        serial: 1
        ptype: "top"
        flipLR: false
        nQuarter: 2
        hdu: 0
        diskCoordSys: "sensor"
        index: 3 0 
    } 
    Amp: { 
        serial: 2
        ptype: "top"
        flipLR: true
        nQuarter: 2
        hdu: 0
        diskCoordSys: "sensor"
        index: 2 0 
    } 
    Amp: { 
        serial: 3
        ptype: "top"
        flipLR: false
        nQuarter: 2
        hdu: 0
        diskCoordSys: "sensor"
        index: 1 0 
    } 
    Amp: { 
        serial: 4
        ptype: "top"
        flipLR: true
        nQuarter: 2
        hdu: 0
        diskCoordSys: "sensor"
        index: 0 0 
    } 
}
